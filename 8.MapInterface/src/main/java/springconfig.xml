<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define a bean for AmeerpetTechnologies -->
    <bean id="institute" class="com.spring.eight.MapInterface.AmeerpetTechnologies">
        <property name="studentCourses">
            <map>
                <entry key="Javafullstack" value="frontend-backend"/>
                <entry key="dotnetfullstack" value="frontend-backend"/>
                 <entry key="python" value="programming language"/>
                
            </map>
        </property>
         
        
    </bean>

</beans>
